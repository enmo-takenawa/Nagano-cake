<div class="container">
  <div class-"row">
    <h2>会員一覧</h2>
  </div>

  <table class="border border-dark">
    <thead>
      <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tr>
    </thead>
    <tbody>
      <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.id %></td>
        <td>
          <%= link_to admins_customer_path(customer) do %>
            <%= f.last_name+f.first_name %>
          <% end %>
        </td>
        <td><%= customer.email %></td>
        <% if customer.is_deleted == true %>
        <td class="text-success">有効</td>
        <% else %>
        <td class="text-success">退会</td>
        <% end %>
      </tr>
      <% end %>
    </tbody>
  </table>
  
</div>
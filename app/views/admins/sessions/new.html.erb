<div class="container">
  <div class="row mt-4 ml-4">
    <h4 class="bg-light text-dark">管理者ログイン</h4>
  </div>

  <div class="row">
      <div class="col">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <table>
          <tr>
            <th class="text-dark" height="50"><%= f.label :email, "メールアドレス" %></th>
            <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
          </tr>
          <tr>
            <th class="text-dark" height="50"><%= f.label :password, "パスワード" %></th>
            <td><%= f.password_field :password, autocomplete: "current-password" %></td>
          </tr>
          <tr>
            <th></th>
            <td><%= f.submit "ログイン", class: "btn btn-success" %></td>
          </tr>
        </table>
        <% end %>
      </div>
   </div>
</div>